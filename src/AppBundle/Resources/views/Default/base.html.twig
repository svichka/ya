  <script>
    // Depend of JQuery
    function gaSend(options) {
      var defaults = {
        event: 'GAEvent',
        eventCategory: null,
        eventAction: null,
        eventLabel: null,
        eventContext: null,
        errorCode: null,
      };
      var gaParams = $.extend({}, defaults, options || {});
      console.log('gaSend', gaParams);
      dataLayer.push(gaParams);
    }

    function fG(e) {
      console.log('val', $(e).val());
      console.log(e.id);
      var name = 'Fields';
      switch (e.id) {
        case 'registration_form_lastname':
          // Выход если короче 2 символов
          if ($(e).val().length < 2) {
            return;
          }
          name = 'Family';
          break;
        case 'registration_form_firstname':
          // Выход если короче 2 символов
          if ($(e).val().length < 2) {
            return;
          }
          name = 'Name';
          break;
        case 'registration_form_birthdate':
          // Если меньше 18 то не отправляем
          if (_calculateAge(moment($(e).val()).toDate()) < 18) {
            return;
          }
          name = 'Birthday';
          break;
        case 'registration_form_email':
          // Выход если не емейл
          if (!validateEmail($(e).val())) {
            return;
          }
          name = 'email';
          break;
        case 'registration_form_regionguid':
          // Выход если не выбран
          if ($(e).val().trim() === '') {
            return;
          }
          name = 'Region';
          break;
        case 'registration_form_cityguid':
          // Выход если не выбран
          if ($(e).val().trim() === '') {
            return;
          }
          name = 'City';
          break;
        case 'registration_form_ismale_0':
        case 'registration_form_ismale_1':
          name = 'Gender';
          // Выход если не заполнено
          if (!$(e).prop("checked")) {
            return;
          }
          break;
        case 'registration_form_password':
        case 'registration_form_confirm_password':
          // Выходим, если длинна меньше 6 или не совпадает повтор пароля
          if ($(e).val().length < 6 || $('#registration_form_password').val() !== $('#registration_form_confirm_password').val()) {
            return;
          }
          name = 'Password';
          break;
        case 'registration_form_isageagreed':
          name = 'Age';
          // Выход если не заполнено
          if (!$(e).prop("checked")) {
            return;
          }
          break;
        case 'registration_form_ispdagreed':
          name = 'Terms';
          // Выход если не заполнено
          if (!$(e).prop("checked")) {
            return;
          }
          break;
      }
      ga('send', 'pageview', '/virtual/' + name);
      var options = {
        event: 'FillingFields',
        eventCategory: name,
        eventAction: 'Filled',
      };
      console.log('fG', options);
      gaSend(options);
    }

    function eA(category, action) {
      console.log('eA', { eventCategory: category, eventAction: action });
      gaSend({ eventCategory: category, eventAction: action });
    }

    function eL(category, action, label) {
      console.log('eL', { eventCategory: category, eventAction: action, eventLabel: label });
      gaSend({ eventCategory: category, eventAction: action, eventLabel: label });
    }

    function eC(category, action, label, context) {
      console.log('eC', { eventCategory: category, eventAction: action, eventLabel: label, eventContext: context });
      gaSend({ eventCategory: category, eventAction: action, eventLabel: label, eventContext: context });
    }

    function eE(category, action, label, context, code) {
      console.log('eE', {
        eventCategory: category,
        eventAction: action,
        eventLabel: label,
        eventContext: context,
        errorCode: code,
      });
      gaSend({
        eventCategory: category,
        eventAction: action,
        eventLabel: label,
        eventContext: context,
        errorCode: code,
      });
    }
  </script>
